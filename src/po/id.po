msgstr "Pilihan argumen-argumen:\n"

#: src/getcert.c:2512 src/getcert.c:2554 src/getcert.c:2623
msgid "* Certificate handling settings:\n"
msgstr "* Keadaan menangani sertifikat:\n"

#: src/getcert.c:2513
msgid "  -I NAME\tnickname to assign to the request\n"
msgstr "-I NAME\tnickname untuk menandai sebuah permintaan\n"

#: src/getcert.c:2514
msgid "  -g SIZE\tsize of key to be generated if one is not already in place\n"
msgstr ""
"-g SIZE\tsize  sebuah kunci untuk menghasilkan jika salah satu telah tidak "
"berada di dalam tempat\n"

#: src/getcert.c:2515 src/getcert.c:2556
msgid ""
"  -r\t\tattempt to renew the certificate when expiration nears (default)\n"
msgstr ""
"-r\t\tattempt untuk memperbarui sertifikat ketika mendekati akhir waktu "
"(kegagalan)\n"

#: src/getcert.c:2516 src/getcert.c:2557
msgid "  -R\t\tdon't attempt to renew the certificate when expiration nears\n"
msgstr ""
"-R\t\tdon't usaha untuk memperbarui sertifikat ketika mendekati akhir waktu\n"

#: src/getcert.c:2518 src/getcert.c:2559
msgid "  -c CA\t\tuse the specified CA rather than the default\n"
msgstr " -c CA\t\tuse CA khusus lebih baik daripada kegagalan\n"

#: src/getcert.c:2520 src/getcert.c:2561 src/getcert.c:2628
msgid ""
"  -T PROFILE\task the CA to process the request using the named profile or "
"template\n"
msgstr ""

#: src/getcert.c:2521
msgid "* Parameters for the signing request:\n"
msgstr "* Beberapa parameter untuk menandai permintaan:\n"

#: src/getcert.c:2522 src/getcert.c:2616
msgid "  -N NAME\tset requested subject name (default: CN=<hostname>)\n"
msgstr "  -N NAME\tset permintaan nama subjek (kegagalan: CN=<hostname>)\n"

#: src/getcert.c:2523 src/getcert.c:2617
msgid "  -U EXTUSAGE\tset requested extended key usage OID\n"
msgstr "-U EXTUSAGE\tset memperluas permintaan pemakaian kunci OID\n"

#: src/getcert.c:2524 src/getcert.c:2618
msgid "  -K NAME\tset requested principal name\n"
msgstr "-K NAME\tset permintaan nama utama\n"

#: src/getcert.c:2525 src/getcert.c:2619
msgid "  -D DNSNAME\tset requested DNS name\n"
msgstr "-D DNSNAME\tset permintaan nama DNS\n"

#: src/getcert.c:2526 src/getcert.c:2620
msgid "  -E EMAIL\tset requested email address\n"
msgstr "-E EMAIL\tset permintaan alamat email\n"

#: src/getcert.c:2527 src/getcert.c:2567 src/getcert.c:2591 src/getcert.c:2629
#: src/getcert.c:2655 src/getcert.c:2670
