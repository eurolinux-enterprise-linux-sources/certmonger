#, c-format
msgid "Usage: %s [-s|-S] [-n|-f] [-d LEVEL] [-p FILE] [-F]\n"
msgstr "Використання: %s [-s|-S] [-n|-f] [-d РІВЕНЬ] [-p ФАЙЛ] [-F]\n"

#: src/main.c:129
msgid "\t-s         use session bus\n"
msgstr "\t-s         використовувати канал даних сеансу\n"

#: src/main.c:130
msgid "\t-S         use system bus\n"
msgstr "\t-S         використовувати канал даних системи\n"

#: src/main.c:131
msgid "\t-n         don't become a daemon\n"
msgstr "\t-n         не запускати у режимі фонової служби\n"

#: src/main.c:132
msgid "\t-f         do become a daemon\n"
msgstr "\t-f         запустити у режимі фонової служби\n"

#: src/main.c:133
msgid "\t-b TIMEOUT bus-activated, idle timeout\n"
msgstr ""
"\t-b ЧАС_ОЧІКУВАННЯ задіяння каналу даних, час очікування у стані "
"бездіяльності\n"

#: src/main.c:134
msgid "\t-B         don't use an idle timeout\n"
msgstr "\t-B         не використовувати час очікування у стані бездіяльності\n"

#: src/main.c:135
msgid "\t-d LEVEL   set debugging level (implies -n)\n"
msgstr "\t-d РІВЕНЬ  встановити рівень діагностики (використовується -n)\n"

#: src/main.c:136
msgid "\t-c COMMAND run COMMAND and exit when it does\n"
msgstr "\t-c КОМАНДА виконати КОМАНДУ і вийти по завершенню її виконання\n"

#: src/main.c:137
msgid "\t-p FILE    write service PID to file\n"
msgstr "\t-p ФАЙЛ    записати PID служби до файла\n"

#: src/main.c:138
msgid "\t-F         force NSS into FIPS mode\n"
msgstr "\t-F         примусово перевести NSS у режим FIPS\n"

