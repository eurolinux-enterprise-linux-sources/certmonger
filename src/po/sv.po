#: src/main.c:114
#, c-format
msgid "Usage: %s [-s|-S] [-n|-f] [-d LEVEL] [-p FILE]\n"
msgstr "Användning: %s [-s|-S] [-n|-f] [-d NIVÅ] [-p FIL]\n"

#: src/main.c:118
msgid "\t-s         use session bus\n"
msgstr "\t-s         använd sessionsbussen\n"

#: src/main.c:119
msgid "\t-S         use system bus\n"
msgstr "\t-S         använd systembussen\n"

#: src/main.c:120
msgid "\t-n         don't become a daemon\n"
msgstr "\t-n         bli inte en demon\n"

#: src/main.c:121
msgid "\t-f         do become a daemon\n"
msgstr "\t-f         bli en demon\n"

#: src/main.c:122
msgid "\t-b TIMEOUT bus-activated, idle timeout\n"
msgstr "\t-b TIDSGRÄNS bussaktiverad, tidsgräns vid inaktivitet\n"

#: src/main.c:123
msgid "\t-B         don't use an idle timeout\n"
msgstr "\t-B         använd inte en tidsgräns vid inaktivitet\n"

#: src/main.c:124
msgid "\t-d LEVEL   set debugging level (implies -n)\n"
msgstr "\t-d NIVÅ    sätt felsökningsnivån (medför -n)\n"

#: src/main.c:125
msgid "\t-p FILE    write service PID to file\n"
msgstr "\t-p FIL     skriv tjänstens PID till filen\n"

