#: src/main.c:137
#, c-format
msgid "Usage: %s [-s|-S] [-n|-f] [-d LEVEL] [-p FILE] [-F]\n"
msgstr "Syntaxe&nbsp;: %s [-s|-S] [-n|-f] [-d NIVEAU] [-p FICHIER] [-F]\n"

#: src/main.c:141
msgid "\t-s         use session bus\n"
msgstr "\t-s         utiliser le bus de la session\n"

#: src/main.c:142
msgid "\t-S         use system bus\n"
msgstr "\t-S         utiliser le bus du système\n"

#: src/main.c:143
msgid "\t-l         start a dedicated listening socket\n"
msgstr ""

#: src/main.c:144
msgid "\t-L         only use a dedicated listening socket\n"
msgstr ""

#: src/main.c:145
msgid "\t-P PATH    specify the dedicated listening socket\n"
msgstr ""

#: src/main.c:146
msgid "\t-n         don't become a daemon\n"
msgstr "\t-n         ne pas se transformer en démon\n"

#: src/main.c:147
msgid "\t-f         do become a daemon\n"
msgstr "\t-f         devenir un démon\n"

#: src/main.c:148
msgid "\t-b TIMEOUT bus-activated, idle timeout\n"
msgstr "\t-b délai d'attente du bus activé, durée d'inactivité\n"

#: src/main.c:149
msgid "\t-B         don't use an idle timeout\n"
msgstr "\t-B         ne pas utiliser un délai d'inactivité\n"

#: src/main.c:150
msgid "\t-d LEVEL   set debugging level (implies -n)\n"
msgstr "\t-d NIVEAU   définir le niveau de débogage (nécessite -n)\n"

#: src/main.c:151
msgid "\t-c COMMAND start COMMAND and exit when it does\n"
msgstr ""

#: src/main.c:152
msgid "\t-p FILE    write service PID to file\n"
msgstr "\t-p FICHIER     écrire le PID du service dans un fichier\n"

#: src/main.c:153
msgid "\t-F         force NSS into FIPS mode\n"
msgstr "\t-F         impose le mode FIPS à NSS\n"

