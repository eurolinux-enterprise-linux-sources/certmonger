#: src/main.c:137
#, c-format
msgid "Usage: %s [-s|-S] [-n|-f] [-d LEVEL] [-p FILE] [-F]\n"
msgstr "Użycie: %s [-s|-S] [-n|-f] [-d POZIOM] [-p PLIK] [-F]\n"

#: src/main.c:141
msgid "\t-s         use session bus\n"
msgstr "\t-s         używa magistrali sesji\n"

#: src/main.c:142
msgid "\t-S         use system bus\n"
msgstr "\t-S         używa magistrali systemowej\n"

#: src/main.c:143
msgid "\t-l         start a dedicated listening socket\n"
msgstr "\t-l         rozpoczyna dedykowane gniazdo nasłuchiwania\n"

#: src/main.c:144
msgid "\t-L         only use a dedicated listening socket\n"
msgstr "\t-L         używa tylko dedykowanego gniazda nasłuchiwania\n"

#: src/main.c:145
msgid "\t-P PATH    specify the dedicated listening socket\n"
msgstr "\t-P ŚCIEŻKA    podaje dedykowane gniazdo nasłuchiwania\n"

#: src/main.c:146
msgid "\t-n         don't become a daemon\n"
msgstr "\t-n         bez zmiany na demona\n"

#: src/main.c:147
msgid "\t-f         do become a daemon\n"
msgstr "\t-f         zmiana na demona\n"

#: src/main.c:148
msgid "\t-b TIMEOUT bus-activated, idle timeout\n"
msgstr ""
"\t-b CZAS    aktywacja przez magistralę, czas oczekiwania na bezczynność\n"

#: src/main.c:149
msgid "\t-B         don't use an idle timeout\n"
msgstr "\t-B         bez użycia czasu oczekiwania na bezczynność\n"

#: src/main.c:150
msgid "\t-d LEVEL   set debugging level (implies -n)\n"
msgstr "\t-d POZIOM  ustawia poziom debugowania (wymusza opcję -n)\n"

#: src/main.c:151
msgid "\t-c COMMAND start COMMAND and exit when it does\n"
msgstr ""
"\t-c POLECENIE rozpoczyna POLECENIE i kończy działanie po jego ukończeniu\n"

#: src/main.c:152
msgid "\t-p FILE    write service PID to file\n"
msgstr "\t-p PLIK    zapisuje PID usługi do pliku\n"

#: src/main.c:153
msgid "\t-F         force NSS into FIPS mode\n"
msgstr "\t-F         wymusza przejście NSS do trybu FIPS\n"

