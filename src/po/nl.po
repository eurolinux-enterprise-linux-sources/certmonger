msgid "\t-b TIMEOUT bus-activated, idle timeout\n"
msgstr "\t-b TIMEOUT bus-geactiveerde, idle timeout\n"

#: src/main.c:149
msgid "\t-B         don't use an idle timeout\n"
msgstr "\t-B         gebruik geen idle timeout\n"

#: src/main.c:150
msgid "\t-d LEVEL   set debugging level (implies -n)\n"
msgstr "\t-d LEVEL   stel debug niveau in (impliceert -n)\n"

#: src/main.c:151
msgid "\t-c COMMAND start COMMAND and exit when it does\n"
msgstr "\t-c COMMANDO start COMMANDO sluit af als deze dat doet\n"

#: src/main.c:152
msgid "\t-p FILE    write service PID to file\n"
msgstr "\t-p BESTAND schrijf een service PID naar bestand\n"

#: src/main.c:153
msgid "\t-F         force NSS into FIPS mode\n"
msgstr "\t-F         forceer NSS naar de FIPS modus\n"

